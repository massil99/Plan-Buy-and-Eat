<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.PlanBuyAndEat.activityTitle" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textSize">40sp</item>
    </style>
    <style name="Theme.PlanBuyAndEat.smalTitle" parent="TextAppearance.AppCompat.Title">
        <item name="android:textSize">25sp</item>
    </style>
    <style name="Theme.PlanBuyAndEat.intputLabel" parent="TextAppearance.AppCompat.Title">
        <item name="android:textSize">20sp</item>
    </style>
    <style name="Theme.PlanBuyAndEat.mediumText" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textSize">17sp</item>
    </style>
    <style name="Theme.PlanBuyAndEat.smalText" parent="TextAppearance.AppCompat.Small">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#aaa</item>
    </style>
    <style name="Theme.PlanBuyAndEat.Button" parent="Widget.AppCompat.Button">
        <item name="android:layout_margin">5dp</item>
        <item name="android:width"> 150dp</item>
        <item name="android:height">55dp</item>
    </style>
    <style name="Theme.PlanBuyAndEat.errorText" parent="TextAppearance.AppCompat.Small">
        <item name="android:textColor">#FF361C</item>
    </style>
    <style name="Theme.PlanBuyAndEat.RightArrowButton" parent="Widget.AppCompat.ImageButton">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:background">?attr/colorPrimary</item>
    </style>

    <style name="Theme.PlanBuyAndEat.XLargeTexti" parent="TextAppearance.AppCompat.Large.Inverse">
        <item name="android:textSize">50sp</item>
    </style>
    <style name="Theme.PlanBuyAndEat.Texti" parent="TextAppearance.AppCompat.Medium.Inverse">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
</resources>